version 39
__lua__
function _init()
	coin_i={1,gen_anim({16,17,18,19},{4,3,3,3})}
end

function _update()

	if coin_i[1] < #coin_i[2]then
		coin_i[1]+=1
	else
		coin_i[1]=1
	end
	
end

function _draw()
	cls()
	rectfill(0,0,127,127,12)
	
	--begin drawing of the map
	c_block=0 --curent block
	for y=0,16 do
		for x=0,16 do
			c_block=mget(x,y)
			if c_block ~= 0 then
				if c_block == 162 then
					spr(coin_i[2][coin_i[1]],x*8,y*8-1)
				else
					spr(c_block,x*8,y*8)
				end
			end
		end
	end
	-- end drawing of the map
	
	rect(0,0,127,127,7)
end
